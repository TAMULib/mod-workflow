        },
        "id" : {
          "type" : "string"
        },
        "name" : {
          "type" : "string",
          "minLength" : 4,
          "maxLength" : 64
        },
        "delegate" : {
          "type" : "string"
        },
        "streaming" : {
          "type" : "boolean"
        },
        "enhancementComparisons" : {
