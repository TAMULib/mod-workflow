{
  "$schema" : "http://json-schema.org/draft-04/schema#",
  "title" : "Workflow",
  "type" : "object",
  "additionalProperties" : false,
  "properties" : {
    "id" : {
      "type" : "string"
    },
    "name" : {
      "type" : "string",
      "minLength" : 4,
      "maxLength" : 64
    },
    "deploymentId" : {
      "type" : "string"
    },
    "active" : {
      "type" : "boolean"
    }
  },
  "required" : [ "name", "active" ]
}